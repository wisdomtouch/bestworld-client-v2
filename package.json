    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "or": "^0.2.0",
    "prettier": "~3.0.2",
    "prettier-eslint": "~15.0.1",
    "stylelint": "~16.2.1",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-standard-scss": "~13.0.0",
