{
  "name": "ketube-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host=0.0.0.0 --port 4320",
    "start:docker": "docker compose up --build admin",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "yarn lint:js && yarn lint:prettier && yarn lint:style",
    "lintfix": "yarn lint:jsfix && yarn lint:prettierfix && lint:stylefix",
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:jsfix": "eslint --ext \".ts\" --ignore-path .gitignore . --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettierfix": "prettier --write --list-different . ",
    "lint:style": "npx stylelint \"src/asset/scss/custom/**/*.scss\" \"src/app/**/*.scss\"",
    "lint:stylefix": "npx stylelint \"src/asset/scss/custom/**/*.scss\" \"src/app/**/*.scss\" --fix",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.0.1",
    "@angular/cdk": "~17.0.1",
    "@angular/common": "~17.0.1",
